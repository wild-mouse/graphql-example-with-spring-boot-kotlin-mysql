type Query { 
  schedules(name: String, allDay: String): [Schedule]
  categories: [Category]
}

type Schedule {
  id: ID
  name: String
  date: String
  allDay: String
  additionalInformation: [AdditionalInformation]
  categories: [Category]
}

type AdditionalInformation {
  id: ID
  scheduleId: String
  name: String
  value: String
}

type Category {
  id: ID
  scheduleId: String
  name: String
}
